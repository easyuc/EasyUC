     | None   => None
     | Some _ =>
         Some {|ke_req2__func = pt1.`1; ke_req2_pt2 = pt2|}
     end.

op epdp_ke_req2 = {|enc = enc_ke_req2; dec = dec_ke_req2|}.

lemma valid_epdp_ke_req2 : valid_epdp epdp_ke_req2.
