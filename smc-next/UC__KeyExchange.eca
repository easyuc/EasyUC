rewrite /epdp_ke_req2 /dec_ke_req2 /enc_ke_req2 /=.
case (mod = Adv \/ pt1.`2 <> pi \/ tag <> 0) => //.
rewrite !negb_or /= not_adv => [#] -> pt1_2 -> match_eq_some /=.
have val_u :
  epdp_unit_univ.`dec u = Some ().
  move : match_eq_some.
  case (epdp_unit_univ.`dec u) => //.
